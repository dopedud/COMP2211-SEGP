<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
    </padding>
    <spacing>10.0</spacing>
    <alignment>CENTER</alignment>

    <Label fx:id="label" text="Label" minWidth="100" />
    <TextField fx:id="textField" maxWidth="60"/>
</fx:root>
