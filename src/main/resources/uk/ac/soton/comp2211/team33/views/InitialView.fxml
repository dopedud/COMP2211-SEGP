<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">
    <Label AnchorPane.topAnchor="10" text="Runway Re-Declaration Tool - Configure a new runway or import an exiting one" />
    <HBox spacing="10" AnchorPane.bottomAnchor="10">
        <Button text="OK" onAction="#configureNew" />
        <Button text="Cancel" onAction="#importExisting" />
    </HBox>
</AnchorPane>
