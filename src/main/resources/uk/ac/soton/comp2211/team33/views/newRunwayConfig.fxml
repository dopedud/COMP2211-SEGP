<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="uk.ac.soton.comp2211.team33.controllers.NewRunwayController"
            prefHeight="400.0" prefWidth="600.0">
    <top>
        <Text text="New Runway Configuration" />
    </top>
    <center>
        <VBox alignment="CENTER">
            <Text text="Enter the runway configuration" />
            <TextField fx:id="runwayName" />
            <TextField fx:id="runwayLength" />
            <Button text="Submit" onAction="#createRunway" />
        </VBox>
    </center>
</BorderPane>
