<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="uk.ac.soton.comp2211.team33.controllers.NewRunwayController"
            prefHeight="400.0" prefWidth="600.0">
    <top>
        <Text text="New Runway Configuration" />
    </top>
    <center>
        <VBox alignment="CENTER">
            <Text text="Enter runway details" />
            <TextField fx:id="runwayDesignator" />
            <TextField fx:id="tora" />
            <TextField fx:id="toda" />
            <TextField fx:id="asda" />
            <TextField fx:id="lda" />
            <TextField fx:id="resa" />
            <TextField fx:id="threshold" />
            <TextField fx:id="clearway" />
            <TextField fx:id="stopway" />
            <TextField fx:id="stripEnd" />
            <TextField fx:id="blastProtection" />
            <Button text="Submit" onAction="#createRunway" />
        </VBox>
    </center>
</BorderPane>
