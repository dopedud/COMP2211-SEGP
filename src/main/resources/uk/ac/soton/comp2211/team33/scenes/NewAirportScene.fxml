<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import uk.ac.soton.comp2211.team33.components.DropdownField?>

<AnchorPane prefWidth="400.0" xmlns="http://javafx.com/javafx/19"
            xmlns:fx="http://javafx.com/fxml/1">
  <Label text="New airport details :" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="20.0"/>
  <VBox AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
    <DropdownField fx:id="city" labelText="City"/>
    <DropdownField fx:id="name" labelText="Name"/>
    <AnchorPane>
      <Button mnemonicParsing="false" text="Submit" onAction="#onSubmitAirport"
              prefHeight="30.0" prefWidth="100.0"
              AnchorPane.topAnchor="20.0" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0"/>
    </AnchorPane>
  </VBox>
</AnchorPane>
